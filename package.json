{
  "name": "exam-archive-new",
  "version": "3.0.0",
  "author": "cxcorp",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:e2e": "npm run db:migrate && NODE_ENV=development npm run db:seed && NODE_ENV=development node dist/index.js",
    "build:scss": "sass --no-source-map -s compressed src/styles/main.scss static/main.css",
    "build:autoprefixer": "postcss -r --use autoprefixer --no-map static/main.css",
    "watch:scss": "sass --watch src/styles/main.scss static/main.css",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "prettier": "prettier --write .",
    "seed-from-files-dev": "cross-env NODE_ENV=development node scripts/seed-db-from-files.js",
    "seed-s3-from-db": "cross-env NODE_ENV=development node scripts/seed-s3-from-db.js",
    "test": "playwright test",
    "test:install": "playwright install --with-deps",
    "test:update-screenshots": "playwright test --update-snapshots",
    "next:dev": "next dev",
    "next:start": "next start",
    "next:build": "next build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/s3-request-presigner": "^3.279.0",
    "classnames": "^2.3.2",
    "content-disposition": "^0.5.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "formidable": "^3.2.5",
    "iron-session": "^6.3.1",
    "knex": "^2.4.2",
    "next": "^13.2.4-canary.2",
    "pg": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slugify": "^1.6.5",
    "transliteration": "^2.3.5",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.6.0",
    "@playwright/test": "^1.31.1",
    "@types/content-disposition": "^0.5.5",
    "@types/formidable": "^2.0.5",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "axe-html-reporter": "^2.2.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
