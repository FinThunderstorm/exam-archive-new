{
  "name": "exam-archive-new",
  "version": "3.0.0",
  "main": "src/index.ts",
  "author": "cxcorp",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run db:migrate && node dist/index.js",
    "start:e2e": "npm run db:migrate && NODE_ENV=development npm run db:seed && NODE_ENV=development node dist/index.js",
    "build": "npm run build:ts && npm run build:views && npm run build:scss && npm run build:autoprefixer",
    "build:ts": "tsc -p tsconfig.json",
    "build:views": "node scripts/build-views.js",
    "build:scss": "sass --no-source-map -s compressed src/styles/main.scss static/main.css",
    "build:autoprefixer": "postcss -r --use autoprefixer --no-map static/main.css",
    "watch": "npm run build && npm run db:migrate && concurrently npm:watch:*",
    "watch:scss": "sass --watch src/styles/main.scss static/main.css",
    "watch:ts": "tsc -p tsconfig.json -w",
    "watch:views": "node scripts/watch-views.js",
    "watch:server": "NODE_ENV=development nodemon -e js,jsx,json --watch dist --delay 0.5 --ignore 'dist/views/**/*.jsx' dist/index.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "prettier": "prettier --write .",
    "seed-from-files-dev": "cross-env NODE_ENV=development node scripts/seed-db-from-files.js",
    "seed-s3-from-db": "cross-env NODE_ENV=development node scripts/seed-s3-from-db.js",
    "test": "playwright test",
    "test:install": "playwright install --with-deps",
    "test:update-screenshots": "playwright test --update-snapshots",
    "next:dev": "next dev",
    "next:start": "next start",
    "next:build": "next build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.267.0",
    "@aws-sdk/cloudfront-signer": "^3.267.0",
    "@aws-sdk/s3-request-presigner": "^3.267.0",
    "axios": "^1.3.2",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "^3.2.5",
    "knex": "^2.4.2",
    "memorystore": "^1.6.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "next": "^13.1.6",
    "pg": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slugify": "^1.6.5",
    "transliteration": "^2.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.6.0",
    "@playwright/test": "^1.30.0",
    "@types/compression": "^1.7.2",
    "@types/content-disposition": "^0.5.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/formidable": "^2.0.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.13.0",
    "@types/react": "18.0.28",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "axe-html-reporter": "^2.2.3",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
