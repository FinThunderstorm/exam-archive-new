{
  "name": "exam-archive-new",
  "version": "2.2.0",
  "main": "src/index.ts",
  "author": "cxcorp",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run db:migrate && node dist/index.js",
    "build": "tsc && cp -R ./src/views ./dist/views && npm run build:scss && npm run build:autoprefixer",
    "build:scss": "sass --no-source-map -s compressed src/styles/main.scss static/main.css",
    "build:autoprefixer": "postcss -r --use autoprefixer --no-map static/main.css",
    "watch": "npm run db:migrate && concurrently npm:watch:scss npm:watch:app",
    "watch:scss": "sass --watch src/styles/main.scss static/main.css",
    "watch:app": "cross-env NODE_ENV=development nodemon -e ts,js,jsx,json --ignore 'src/views/**/*.jsx' --exec ts-node src/index.ts",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "prettier": "prettier --write .",
    "seed-from-files-dev": "cross-env NODE_ENV=development node scripts/seed-db-from-files.js",
    "seed-s3-from-db": "cross-env NODE_ENV=development node scripts/seed-s3-from-db.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.202.0",
    "@aws-sdk/cloudfront-signer": "^3.201.0",
    "@aws-sdk/s3-request-presigner": "^3.202.0",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.3",
    "knex": "^2.3.0",
    "memorystore": "^1.6.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "pg": "^8.8.0",
    "prop-types": "^15.8.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slugify": "^1.6.5",
    "transliteration": "^2.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/content-disposition": "^0.5.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
